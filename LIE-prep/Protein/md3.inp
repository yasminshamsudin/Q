[MD]
steps                  500000
stepsize                  2.00
temperature             298.0
bath_coupling           100
shake_solvent            on
shake_solute             on
separate_scaling         on
lrf                      on

[cut_offs]
solute_solute            10
solute_solvent           10
solvent_solvent          10
q_atom                   99
lrf                      99

[sphere]
shell_force              10
shell_radius           0.83

[intervals]
non_bond                25
output                  25
energy                  25
trajectory             2500

[trajectory_atoms]
not excluded

[files]
topology       ../LIG.protein.top
final          md3.re
energy         md3.en
restart        md2.re
fep            ../LIG.fep
trajectory     md3.dcd

