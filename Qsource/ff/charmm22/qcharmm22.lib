{ALA}                 ! Alanine
    [info]
        SYBYLtype  RESIDUE     !SYBYL substructure type
    [atoms]
         1 N      NH1         -0.470 !At. no., name, iac, charge
         2 HN     H            0.310
         3 CA     CT1          0.070
         4 HA     HB           0.090
         5 CB     CT3         -0.270
         6 HB1    HA           0.090
         7 HB2    HA           0.090
         8 HB3    HA           0.090
         9 C      C            0.510
        10 O      O           -0.510
    [bonds]
       CB   CA                !connect i -- j
       N    HN
       N    CA
       O    C
       C    CA
       CA   HA
       CB   HB1
       CB   HB2
       CB   HB3
    [connections]   !how to bond to previous and next
        head     N  
        tail     C  
    [impropers]
         N     -C     CA     HN
         C     CA     +N      O
    [charge_groups] !charge groups, with switch atom first
        N    HN   CA   HA 
        CB   HB1  HB2  HB3
        C    O  
